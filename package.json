{
  "name": "next-core-object",
  "version": "2.5.5",
  "description": "The Augmented.js Next - Object and Events.",
  "main": "dist/next-core-object.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/index.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/next-core-object.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^3.2.2",
    "eslint-config-kentcdodds": "^9.0.2",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.2",
    "jsdom": "^11.12.0",
    "mocha": "^4.1.0",
    "mocha-jsdom": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "lodash.bind": "^4.2.1",
    "next-core-utilities": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/next-core-object/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
